<body>
<div class="container">
  <h1 class="text-center" style="font-weight: bold">Add Property</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for(@property) do |f| %>
          <%= render 'shared/error_messages_html' %>

          <%= f.label :house_no %>
          <%= f.text_field :house_no, class: 'form-control' %>

          <%= f.label :street %>
          <%= f.text_field :last_name, class: 'form-control' %>

          <%= f.label :suburbs %>
          <%= f.text_field :email, class: 'form-control' %>

          <%= f.label :postcode %>
          <%= f.text_field :password, class: 'form-control' %>

          <%= f.label :price %>
          <%= f.number_field :price, class: 'form-control' %>

          <div>
            <%= f.submit "Create my account", class: "btn btn-primary btn-block" %>
          </div>
      <% end %>
    </div>
  </div>
</div>
</body>

<%= render 'form', property: @property %>

<%= link_to 'Back', properties_path %>
